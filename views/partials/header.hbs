<header class="header">
    <div class="conteiner header__conteiner">
      <a class="logo" href="/">Web<span class="logo__text--black">Studio</span></a>
      <!--Навигация по сайту-->
      <div class="mobile-wrap" id="mobile-wrap" data-menu>
        {{> site-nav}}
        {{> networks}}
        {{> contacts}}
      </div>
      <button
        class="button-mobile"
        type="button"
        aria-expanded="false"
        aria-controls="mobile-wrap"
        data-menu-button>
        <svg width="40" height="40" aria-label="Переключатель мобильного меню">
          <use class="button-mobile__icon-menu" href=./images/icons.svg#icon-menu></use>
          <use class="button-mobile__icon-close" href=./images/icons.svg#icon-close></use>
        </svg>
      </button>
    </div>
</header>
